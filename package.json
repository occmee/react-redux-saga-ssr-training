{
  "name": "second-app",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:relationslab/second-app.git",
  "scripts": {
    "start": "node ./src/server/index.js",
    "preserver:build": "rm -fr ./build/server",
    "server:build": "tsc -p .",
    "server:codecheck": "yarn server:lint && yarn server:test",
    "server:lint": "tslint -c tslint.json --project tsconfig.json",
    "server:watch": "tsc -w -p .",
    "server:test": "jest --testPathPattern \"build/server/.*/__tests__/.*\\.jsx?$\"",
    "server:test:watch": "jest --testPathPattern \"build/server/.*/__tests__/.*\\.jsx?$\" --watch",
    "preserver:cli": "tsc -p ./tsconfig.script.json",
    "server:cli": "node ./build/server/script/command/cli.js",
    "predb:migrate": "tsc -p ./tsconfig.script.json",
    "db:migrate": "sequelize db:migrate",
    "common:test": "jest --testPathPattern \"src/common/.*/__tests__/.*\\.jsx?$\"",
    "client:start": "webpack-dev-server --inline --hot --host 0.0.0.0",
    "client:build": "NODE_ENV=development webpack",
    "client:test": "jest --testPathPattern \"src/client/app/.*/__tests__/.*\\.jsx?$\"",
    "client:storybook": "start-storybook -p 9001 -c ./src/client/storybook"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "ejs": "^2.5.5",
    "email-templates": "^2.5.4",
    "graphql": "^0.9.1",
    "graphql-custom-datetype": "^0.3.0",
    "graphql-server-core": "0.5.0",
    "graphql-server-koa": "0.5.0",
    "graphql-subscriptions": "^0.2.3",
    "graphql-tools": "^0.9.2",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^3.2.1",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.11.5",
    "koa-helmet": "^3.1.0",
    "koa-logger": "^2.0.0",
    "koa-passport": "3.0.0-rc.3",
    "koa-router": "^7.1.0",
    "lodash": "^4.15.0",
    "moment": "^2.15.1",
    "moment-timezone": "^0.5.11",
    "mysql": "^2.13.0",
    "nodemailer": "^2.7.2",
    "passport-jwt": "^2.2.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "react-s-alert": "^1.2.2",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.1",
    "reflect-metadata": "^0.1.9",
    "sequelize": "^3.30.0",
    "tslib": "^1.5.0",
    "validator": "^5.7.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-3": "^6.11.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.25.0",
    "dotenv-webpack": "^1.3.2",
    "eslint": "^1.4.0",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "^0.13.0",
    "eslint-plugin-react": "^4.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "json-loader": "^0.5.4",
    "nib": "^1.1.2",
    "pleeease-loader": "^1.0.0",
    "remap-istanbul": "^0.8.4",
    "sequelize-cli": "^2.5.1",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.3.1",
    "supertest": "^3.0.0",
    "svg-loader": "0.0.2",
    "svg-stylus": "0.0.5",
    "url-loader": "^0.5.7",
    "vorpal": "^1.11.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  }
}
