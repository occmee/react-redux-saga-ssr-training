{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      "double"
    ],
    "no-reference": true,
    "object-literal-sort-keys": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "arrow-parens": true,
    "no-var-requires": false,
    "ordered-imports": [
      false
    ],
    "only-arrow-functions": [
      false
    ],
    "max-classes-per-file": [
      false
    ]
  }
}